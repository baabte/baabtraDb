db.system.js.save({_id:"fnLoadMenteesForApprove",value:function(e,r,t,s,n){var o={};return o.type="regex",o.orderFroms=db.clnTrainingRequest.find({$or:[{status:{$regex:new RegExp(n,"i")}},{"requesteeDetails.eMail":{$regex:new RegExp(n,"i")}},{customCompanyCode:{$regex:new RegExp(n,"i")}},{"requesteeDetails.type":{$regex:new RegExp(n,"i")}}],companyId:ObjectId(e),"orderDetails.userInfo.status":{$in:r}}).skip(t>0?(t-1)*s:0).sort({customCompanyCode:-1}).limit(s).toArray(),o}});
